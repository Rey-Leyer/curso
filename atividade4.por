programa {
  funcao inicio() {
    
    inteiro numero1,numero2

    escreva("Informe o primeiro numero: ")
    leia(numero1)
    escreva("Informe o segundo numero: ")
    leia(numero2)

    se(numero1>numero2)
    {
      escreva("O primeiro numero é maior que  o segundo")
    }
    senao
    {
      escreva("O segundo numero é maior que  o primeiro")
    }
  }
}
